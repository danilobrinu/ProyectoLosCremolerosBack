@model IEnumerable< UTP.PortalEmpleabilidad.Modelo.Evento>
<div class="col-sm">   
    @foreach (var item in Model)
    {
        <div class="row grilla1-linea fuente-pequeña">
            <div class="col-sm-10">
                @*@Html.ActionLink(item.NombreEvento, "Eventos", "Alumno", new { id = item.IdEvento }, htmlAttributes: new { target = "_blank" })*@
                @Html.ActionLink(item.NombreEvento, "Eventos", "Alumno", new { id = item.IdEvento }, null)
            </div>
            <div class="col-sm-10">
                @Html.DisplayFor(modelItem => item.FechaEventoTexto)
            </div>
            <div class="col-sm-10">
               @Html.DisplayFor(modelItem => item.LugarEvento)
            </div>
        </div>
        <hr />

    }

</div>
