
@{
    ViewBag.Title = "ObtenerListaOpciones";
}
<h2>@ViewBag.Message</h2>
<h2>ObtenerListaOpciones</h2>




@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>VistaComboAlumno</h4>
        <hr />
        @Html.ValidationSummary(true)

        <div class="form-group">
            @Html.Label("Tipo de Estudio", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
            @*tipo de estudio*@
            @Html.DropDownList("Tipo de Estudio", (SelectList)ViewBag.DropDownValues7)


            </div>
        </div>
        <div class="form-group">
            @Html.Label("Carrera profesional", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
             @*Carrera profesional*@
             @Html.DropDownList("Carrera profesional", (SelectList)ViewBag.DropDownValues2)


            </div>
        </div>
       

    </div>
}