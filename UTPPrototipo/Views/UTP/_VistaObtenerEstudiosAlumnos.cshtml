@model IEnumerable<UTP.PortalEmpleabilidad.Modelo.AlumnoEstudio>

<h6>Estudios</h6>

<div class="row grilla1-cabecera color-paleta3">
    <div class="col-sm-1">
        Institución
    </div>
    <div class="col-sm-1">
        Estudio
    </div>
    <div class="col-sm-1">
        Tipo
    </div>
    <div class="col-sm-1">
        Estado
    </div>
    <div class="col-sm-1">
        Observación
    </div>
    <div class="col-sm-1">
        Fecha Inicio
    </div>
    <div class="col-sm-1">
        Feha Fin
    </div>
    <div class="col-sm-1">
        Ciclo Equivalente
    </div>
    <div class="col-sm-1">
        Dato UTP
    </div>
    <div class="col-sm-1">
        Estado
    </div>
    <div class="col-sm-1">
        Creado
    </div>
    <div class="col-sm-1">
        Modificado
    </div>
</div>

@foreach (var item in @Model)
{

<div class="row grilla1-linea">
    <div class="col-sm-1">
        @item.Institucion
    </div>
    <div class="col-sm-1">
       @item.Estudio
    </div>
    <div class="col-sm-1">
        @item.TipoDeEstudio    
    </div>
    <div class="col-sm-1">
        @item.EstadoDelEstudio
    </div>
    <div class="col-sm-1">
        @item.Observacion
    </div>
    <div class="col-sm-1">
        @item.FechaInicioMes/@item.FechaInicioAno
    </div>
    <div class="col-sm-1">
        @item.FechaFinMes/@item.FechaFinAno
    </div>
    <div class="col-sm-1">
        @item.CicloEquivalente
    </div>
    <div class="col-sm-1">
        @item.DatoUTP
    </div>
    <div class="col-sm-1">
        @item.Estado
    </div>
    <div class="col-sm-1">
        @item.CreadoPor @item.FechaCreacion
    </div>
    <div class="col-sm-1">
        @item.ModificadoPor @item.FechaModificacion
    </div>
</div>
}
<hr />