@model IEnumerable<UTP.PortalEmpleabilidad.Modelo.UtpAlumnoDetalle>

<h6>Estudios</h6>

<div class="row grilla1-cabecera color-paleta3">
    <div class="col-sm-2">
        Institución
    </div>
    <div class="col-sm-2">
        Estudio
    </div>
    <div class="col-sm-1">
        Tipo
    </div>
    <div class="col-sm-1">
        Estado
    </div>
    <div class="col-sm-1">
        Observación
    </div>
    <div class="col-sm-1">
        Fecha Inicio
    </div>
    <div class="col-sm-1">
        Feha Fin
    </div>
    <div class="col-sm-1">
        Ciclo Equivalente
    </div>
    <div class="col-sm-1">
        Dato UTP
    </div>
    <div class="col-sm-1">
        Estado
    </div>
</div>

@foreach (var item in @Model)
{

<div class="row grilla1-linea">
    <div class="col-sm-3">
      
        @item.Carrera
    </div>
    <div class="col-sm-1">
       @item.Estudio
    </div>
    <div class="col-sm-2">
        @item.EstadoEstudio    
    </div>
    <div class="col-sm-1">
        @item.CicloEquivalente 
    </div>
  

</div>
}
<hr />