@model UTP.PortalEmpleabilidad.Modelo.Evento

@{
    Layout = "~/Views/Empresa/_LayoutEmpresa.cshtml";
    ViewBag.Title = "Eventos";
}

<div class="container fuente-pequeña">

    <div class="row">
        <div class="col-sm-6">
            <h6>Eventos Próximos</h6>
            <hr />
            <div class="row grilla1-cabecera">
                <div class="col-sm-3">
                    Evento
                </div>
                <div class="col-sm-3">
                    Fecha
                </div>
                <div class="col-sm-3">
                    Lugar
                </div>
                <div class="col-sm-3">
                    Organiza
                </div>
            </div>
            <div class="row grilla1-linea">
                <div class="col-sm-3">
                    @Model.NombreEvento
                </div>
                <div class="col-sm-3">
                    Fecha
                </div>
                <div class="col-sm-3">
                    Lugar
                </div>
                <div class="col-sm-3">
                    Organiza
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        $("#divFinalCabeceraEmpresa").removeClass("color-paleta1 color-paleta2 color-paleta3 color-paleta4").addClass("color-paleta5");
    });
</script>